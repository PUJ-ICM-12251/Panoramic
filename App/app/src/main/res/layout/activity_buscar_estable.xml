<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_grey"
    android:padding="15dp"
    tools:context=".BuscarEstable">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/button8"
        android:text="¿A dónde vamos?"
        android:textColor="#FFFFFF"
        android:textSize="50dp"
        android:textStyle="bold"
        android:layout_marginBottom="8dp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="41dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView5"
        android:layout_marginTop="10dp"
        android:text="Populares:"
        android:textColor="#FFFFFF"
        android:textSize="35dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="41dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="x:"
        android:textColor="#0232323F"
        android:textSize="10dp"
        android:textStyle="bold"
        tools:ignore="TextSizeCheck" />


    <Button
        android:id="@+id/button3"
        android:layout_width="181dp"
        android:layout_height="70dp"
        android:layout_above="@id/textView5"
        android:layout_marginRight="5dp"
        android:text="Restaurantes"
        app:icon="@drawable/resic"
        app:iconSize="20dp"
        app:iconTint="@null"
        app:backgroundTint="@color/subdued_red"
        android:background="@drawable/botones"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="149dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="181dp"
        android:layout_height="70dp"
        android:layout_above="@id/textView5"
        android:text="Eventos"
        app:icon="@drawable/eveic"
        app:iconSize="50dp"
        app:iconTint="@null"
        app:backgroundTint="@color/subdued_red"
        android:background="@drawable/botones"
        android:layout_toRightOf="@id/button3"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="149dp" />


    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/button10"
        android:layout_marginBottom="5dp"
        android:width="368dp"
        android:height="40dp"
        android:background="@drawable/botones"
        android:text="Busca tu establecimiento favorito"
        android:textAlignment="viewStart"
        app:icon="@drawable/busq"
        app:iconSize="30dp"
        app:iconTint="@null"
        android:textColor="@color/black"
        android:textSize="10dp"
        android:textStyle="bold"
        app:backgroundTint="#EAE8E8"
        tools:ignore="TextSizeCheck"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="149dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_above="@id/button3"
        android:layout_marginTop="10dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="10dp"
        android:width="20dp"
        android:background="@drawable/botones"
        android:text="Popular"
        android:textSize="7dp"
        app:icon="@drawable/popic"
        app:iconSize="13dp"
        app:iconTint="@null"
        app:backgroundTint="@color/subdued_red"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/button11"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_above="@id/button3"
        android:layout_marginTop="10dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="10dp"
        android:layout_toRightOf="@id/button10"
        android:width="20dp"
        android:background="@drawable/botones"
        android:text="Filtros"
        android:textSize="7dp"
        app:icon="@drawable/filic"
        app:iconSize="13dp"
        app:iconTint="@null"
        app:backgroundTint="@color/subdued_red"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="217dp" />

    <Button
        android:id="@+id/button12"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_above="@id/button3"
        android:layout_marginTop="10dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="10dp"
        android:layout_toRightOf="@id/button11"
        android:width="20dp"
        android:height="5dp"
        android:background="@drawable/botones"
        android:text="Precio"
        android:textSize="8dp"
        app:icon="@drawable/precic"
        app:iconSize="13dp"
        app:iconTint="@null"
        app:backgroundTint="@color/subdued_red"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="217dp" />

    <Button
        android:id="@+id/button13"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_above="@id/button3"
        android:layout_marginTop="10dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="10dp"
        android:layout_toRightOf="@id/button12"
        android:width="10dp"
        android:background="@drawable/botones"
        android:text="Ranking"
        android:textSize="7dp"
        app:icon="@drawable/rankic"
        app:iconSize="13dp"
        app:iconTint="@null"
        app:backgroundTint="@color/subdued_red"
        tools:ignore="TextSizeCheck,TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="217dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="180dp"
        android:layout_height="133dp"
        android:layout_below="@id/textView3"
        android:layout_marginTop="20dp"
        android:layout_marginRight="5dp"
        app:srcCompat="@drawable/andresdc" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="180dp"
        android:layout_height="133dp"
        android:layout_below="@id/textView3"
        android:layout_marginTop="20dp"
        android:layout_toRightOf="@id/textView5"
        app:srcCompat="@drawable/cantina" />

    <Button
        android:id="@+id/button14"
        android:layout_width="181dp"
        android:layout_height="80dp"
        android:layout_below="@id/imageView3"
        android:text="Andrés D.C.
        Calle 82 #12-21 Bogotá"
        app:backgroundTint="#454242" />

    <Button
        android:id="@+id/button15"
        android:layout_width="181dp"
        android:layout_height="80dp"
        android:layout_toRightOf="@id/textView5"
        app:backgroundTint="#454242"
        android:layout_below="@id/imageView4"
        android:text="Cantina la 15 Calle 58 #10-32 Bogotá" />

</RelativeLayout>